{
  "manifest_version": 2,
  "name": "Unit Converter",
  "version": "1.0",
  "description": "Convert units",
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
